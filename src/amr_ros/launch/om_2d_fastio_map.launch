<?xml version="1.0"?>
<launch>

  <node name="pointcloud_to_laserscan" pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" output="screen">
    <remap from="cloud_in" to="/cloud_registered_body"/>
    <rosparam command="load" file="$(find amr_ros)/config/point_livox_fastlio.yaml"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="map_to_camera_init" args="0 0 0 0 0 0 map camera_init 100"/>

    <!-- gmapping -->
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" clear_params="true">
    <rosparam command="load" file="$(find amr_ros)/config/gmapping.yaml" />
    <remap from="scan" to="scan" />
    <param name="base_frame" value="body" />
    <param name="odom_frame" value="camera_init" />
    <param name="map_frame"  value="map" />
  </node>

  <!--FAST-LIO-->
  <include file="$(find fast_lio)/launch/mapping_mid360.launch"/>
  <!--Livox Lidar -->
  <include file="$(find livox_ros_driver2)/launch_ROS1/msg_MID360.launch" />


  <arg name="gui" default="true" />
  <arg name="rvizconfig" default="$(find amr_ros)/rviz/livox.rviz" />
  <node pkg="rviz" type="rviz" name="rviz_fastlio_map" args="-d $(arg rvizconfig)" required="true" />

</launch>
